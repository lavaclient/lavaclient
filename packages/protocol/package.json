{
    "name": "lavalink-protocol",
    "version": "1.0.0-rc.6",
    "description": "Type-safe validation for the Lavalink protocol.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "maintainers": [
        {
            "email": "hi@vzt.gay",
            "name": "vzt",
            "url": "https://vzt.gay"
        }
    ],
    "files": [
        "dist/*",
        "!dist/.tsbuildinfo"
    ],
    "scripts": {
        "build": "tsc",
        "check": "tsc --noEmit",
        "format": "prettier -w src/**.ts",
        "prepublishOnly": "pnpm run format && pnpm run build"
    },
    "devDependencies": {
        "@lavaclient/config-prettier": "workspace:^",
        "@lavaclient/config-typescript": "workspace:^",
        "@types/node": "^20.3.3",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "@effect/schema": "^0.49.4",
        "tslib": "^2.6.0"
    },
    "engines": {
        "node": ">=20"
    }
}
